{% extends "layout.j2" %}

{% set page = "timetable" %}

{% block content %}
  <main class="page-main bg">
    <div class="breadcumbs">
      <div class="breadcumbs__container container">
        <ul class="breadcumbs__list">
          <li class="breadcumbs__item">
            <a class="breadcumbs__link" href="./index.html">Главная</a>
            <svg class="breadcumbs__arrow" width="6" height="9" viewBox="0 0 6 9">
              <use xlink:href="img/sprite.svg#breadcumbs-arrow"></use>
            </svg>
          </li>
          <li class="breadcumbs__item">
            <span class="breadcumbs__link breadcumbs__link--current">Расписание клуба</span>
          </li>
        </ul>
      </div>
    </div>

    <section class="timetable pads timetable--own-page">
      <div class="container">
        <div class="timetable__content">
          <h1 class="timetable__title page__title">Расписание клуба</h1>
          <p>Здесь должно быть расписание</p>
        </div>
      </div>
    </section>

  </main>

{% endblock content %}

{% block js %}
  <script src="./js/main.min.js"></script>
{% endblock %}
