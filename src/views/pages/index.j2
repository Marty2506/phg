{% extends "layout.j2" %}

{% set page = "index" %}

{% block content %}
  <main class="page-main">
    <section class="video-header">
      <h1 class="visually-hidden">Главная Powerhouse gym</h1>
      <div class="container">
        <div class="video-header__content">
          <span class="video-header__emphasis">В фитнес-клубе</span>
          <span class="video-header__emphasis video-header__emphasis--red">Powerhouse gym</span>
          <p class="video-header__text">есть все для достижения максимальных спортивных результатов</p>
          <div class="video-header__buttons">
            <a class="button video-header__button button video-header__button--red" href="#!">от 66 ₽ в день</a>
            <a class="button video-header__button" href="#!">
              <svg class="video-header__button-icon button__icon" width="35" height="40" viewBox="0 0 35 40">
                <use xlink:href="img/sprite.svg#play-icon"></use>
              </svg>
              <span>тур по клубу</span>
            </a>
          </div>
        </div>
      </div>
      <video class="video-header__video" preload="none" loop muted poster="@img/video-header-bg.jpg">
        <source src="./files/gym-video.mp4" type="video/mp4">
      </video>
    </section>

    {% set gallerySwiperCnt = 1 %}
    <section class="gym-info">
      <h2 class="visually-hidden">Информация о тренажерном зале</h2>
      <div class="gym-info__tabs tabs">
        <div class="gym-info__tabs-header">
          <div class="container gym-info__tabs-header-container">
            <ul class="gym-info__nav tabs__list" role="tablist">
              <li class="gym-info__nav-item tabs__item tabs__item" role="presentation">
                <a class="gym-info__nav-link tabs__button active" href="#free-weights" aria-controls="free-weights" role="tab" data-toggle="tab">
                  <svg class="gym-info__nav-link-icon" width="46" height="25" viewBox="0 0 46 25">
                    <use xlink:href="img/sprite.svg#free-weights-icon"></use>
                  </svg>
                  <span>Свободные веса</span>
                </a>
              </li>
              <li class="gym-info__nav-item tabs__item tabs__item" role="presentation">
                <a class="gym-info__nav-link tabs__button" href="#gym" aria-controls="gym" role="tab" data-toggle="tab">
                  <svg class="gym-info__nav-link-icon" width="35" height="37" viewBox="0 0 35 37">
                    <use xlink:href="img/sprite.svg#gym-icon"></use>
                  </svg>
                  <span>Тренажерный зал</span>
                </a>
              </li>
              <li class="gym-info__nav-item tabs__item tabs__item" role="presentation">
                <a class="gym-info__nav-link tabs__button" href="#crossfit-studio" aria-controls="crossfit-studio" role="tab" data-toggle="tab">
                  <svg class="gym-info__nav-link-icon" width="34" height="34" viewBox="0 0 34 34">
                    <use xlink:href="img/sprite.svg#crossfit-studio-icon"></use>
                  </svg>
                  <span>Студия кроссфита</span>
                </a>
              </li>
              <li class="gym-info__nav-item tabs__item tabs__item" role="presentation">
                <a class="gym-info__nav-link tabs__button" href="#cycle-studio" aria-controls="cycle-studio" role="tab" data-toggle="tab">
                  <svg class="gym-info__nav-link-icon" width="36" height="34" viewBox="0 0 36 34">
                    <use xlink:href="img/sprite.svg#cycle-studio-icon"></use>
                  </svg>
                  <span>Студия сайкл</span>
                </a>
              </li>
              <li class="gym-info__nav-item tabs__item tabs__item" role="presentation">
                <a class="gym-info__nav-link tabs__button" href="#trainings" aria-controls="trainings" role="tab" data-toggle="tab">
                  <svg class="gym-info__nav-link-icon" width="43" height="37" viewBox="0 0 43 37">
                    <use xlink:href="img/sprite.svg#trainings-icon"></use>
                  </svg>
                  <span>Тренировки LesMills</span>
                </a>
              </li>
              <li class="gym-info__nav-item tabs__item tabs__item" role="presentation">
                <a class="gym-info__nav-link tabs__button" href="#cardio-zone" aria-controls="cardio-zone" role="tab" data-toggle="tab">
                  <svg class="gym-info__nav-link-icon" width="36" height="37" viewBox="0 0 36 37">
                    <use xlink:href="img/sprite.svg#cardio-icon"></use>
                  </svg>
                  <span>Зона кардио</span>
                </a>
              </li>
              <li class="gym-info__nav-item tabs__item tabs__item" role="presentation">
                <a class="gym-info__nav-link tabs__button" href="#martial-arts-zone" aria-controls="martial-arts-zone" role="tab" data-toggle="tab">
                  <svg class="gym-info__nav-link-icon" width="28" height="37" viewBox="0 0 28 37">
                    <use xlink:href="img/sprite.svg#martial-arts-zone-icon"></use>
                  </svg>
                  <span>Зона единоборств</span>
                </a>
              </li>
            </ul>
          </div>
        </div>
        <div class="gym-info__tabs-content tabs__content">
          <div class="gym-info__tabs-content-container container">
            <div class="gym-info__panel tabs__panel active" role="tabpanel" id="free-weights">
              <h3 class="visually-hidden">Свободные веса</h3>
              <div class="gym-info__panel-wrapper">
                <div class="gym-info__panel-main">
                  <span class="gym-info__bg-text">Веса</span>
                  <p class="gym-info__panel-title">
                    <span>В зоне свободных весов</span> каждый найдет&nbsp;то, что нужно
                  </p>
                  <ul class="gym-info__panel-list list">
                    <li class="list__item"><b>7 скамеек для жима лежа</b> — всегда есть свободная</li>
                    <li class="list__item"><b>Помосты для тяжелоатлетов</b> и пауэрлифтеров. Блины от&nbsp;1 до 50&nbsp;кг</li>
                    <li class="list__item"><b>7 отдельных стоек для приседаний</b> и 4 в зоне кроссфита</li>
                  </ul>
                  <div class="gym-info__panel-buttons">
                    <a class="button button--reverse" href="#!">Записаться на пробную тренировку</a>
                    <a class="button" href="./free-weights-zone.html">
                      <span>Подробнее о зале</span>
                      <svg class="button__icon" width="8" height="14" viewBox="0 0 8 14">
                        <use xlink:href="img/sprite.svg#button-arrow-icon"></use>
                      </svg>
                    </a>
                  </div>
                </div>
                <div class="gym-info__panel-gallery gallery">
                  {% set SLIDES_NUM = 10 %}
                  <div class="gallery__swiper-wrapper">
                    <div class="gallery__swiper gallery__swiper--main swiper">
                      <button class="gallery__swiper-button gallery__swiper-button--prev" id="gallery-swiper-{{gallerySwiperCnt}}-prev" type="button">
                        <svg class="gallery__swiper-button-icon" width="11" height="17" viewBox="0 0 11 17">
                          <use xlink:href="img/sprite.svg#swiper-arrow-icon"></use>
                        </svg>
                        <span class="visually-hidden">Перелестнуть назад</span>
                      </button>
                      <button class="gallery__swiper-button gallery__swiper-button--next" id="gallery-swiper-{{gallerySwiperCnt}}-next" type="button">
                        <svg class="gallery__swiper-button-icon" width="11" height="17" viewBox="0 0 11 17">
                          <use xlink:href="img/sprite.svg#swiper-arrow-icon"></use>
                        </svg>
                        <span class="visually-hidden">Перелестнуть вперед</span>
                      </button>
                      <div class="swiper-wrapper">
                        {% for imageCount in range(SLIDES_NUM) %}
                          <div class="gallery__swiper-slide swiper-slide">
                            <picture class="gallery__swiper-slide-wrapper">
                              <source type="image/webp" srcset="https://loremflickr.com/1920/1080/gym,girl/all?{{imageCount + 1}} 1x, https://loremflickr.com/1920/1080/gym,girl/all?{{imageCount + 1}} 2x">
                              <img class="gallery__image" src="https://loremflickr.com/1920/1080/gym,girl/all?{{imageCount + 1}}" srcset="https://loremflickr.com/1920/1080/gym,girl/all?{{imageCount + 1}} 2x" alt="Описание фото {{imageCount + 1}}" loading="lazy" width="1170" height="658">
                            </picture>
                          </div>
                        {% endfor %}
                      </div>
                    </div>
                    <div class="gallery__swiper gallery__swiper--thumbs swiper">
                      <div class="swiper-wrapper">
                        {% for imageCount in range(SLIDES_NUM) %}
                          <div class="gallery__swiper-thumb swiper-slide">
                            <picture class="gallery__swiper-thumb-wrapper">
                              <source type="image/webp" srcset="https://loremflickr.com/1920/1080/gym,girl/all?{{imageCount + 1}} 1x, https://loremflickr.com/1920/1080/gym,girl/all?{{imageCount + 1}} 2x">
                              <img class="gallery__image" src="https://loremflickr.com/1920/1080/gym,girl/all?{{imageCount + 1}}" srcset="https://loremflickr.com/1920/1080/gym,girl/all?{{imageCount + 1}} 2x" alt="Описание фото {{imageCount + 1}}" loading="lazy" width="146" height="100">
                            </picture>
                          </div>
                        {% endfor %}
                      </div>
                    </div>
                  </div>
                </div>
                {% set gallerySwiperCnt = gallerySwiperCnt + 1 %}
              </div>
            </div>
            <div class="gym-info__panel tabs__panel" role="tabpanel" id="gym">
              <h3 class="visually-hidden">Тренажерный зал</h3>
              <div class="gym-info__panel-wrapper">
                <div class="gym-info__panel-main">
                  <span class="gym-info__bg-text">Трен</span>
                  <p class="gym-info__panel-title">
                    Премиальные тренажеры
                  </p>
                  <div class="gym-info__panel-logos">
                    <div class="gym-info__panel-logos-item">
                      <picture>
                        <source type="image/webp" srcset="@img/simulators/life-fitness-logo.webp 1x">
                        <img class="gym-info__panel-logo-image" src="@img/simulators/life-fitness-logo.png" alt="Логотип life-fitness" width="182" height="53">
                      </picture>
                    </div>
                    <div class="gym-info__panel-logos-item">
                      <picture>
                        <source type="image/webp" srcset="@img/simulators/hammer-strength-logo.webp 1x">
                        <img class="gym-info__panel-logo-image" src="@img/simulators/hammer-strength-logo.png" alt="Логотип hammer-strength" width="111" height="43">
                      </picture>
                    </div>
                    <div class="gym-info__panel-logos-item">
                      <picture>
                        <source type="image/webp" srcset="@img/simulators/you-steel-logo.webp 1x">
                        <img class="gym-info__panel-logo-image" src="@img/simulators/you-steel-logo.png" alt="Логотип yousteel" width="94" height="49">
                      </picture>
                    </div>
                  </div>
                  <p class="gym-info__panel-text">
                    Обеспечивают правильную технику исполнения безопасность упражнений. В итоге вы работаете эффективней и сводите к нулю риск получить травму
                  </p>
                  <div class="gym-info__panel-buttons">
                    <a class="button button--reverse" href="#!">Записаться на пробную тренировку</a>
                    <a class="button" href="./train-equip.html">
                      <span>Подробнее о тренажерах</span>
                      <svg class="button__icon" width="8" height="14" viewBox="0 0 8 14">
                        <use xlink:href="img/sprite.svg#button-arrow-icon"></use>
                      </svg>
                    </a>
                  </div>
                </div>
                <div class="gym-info__panel-gallery gallery">
                  {% set SLIDES_NUM = 10 %}
                  <div class="gallery__swiper-wrapper">
                    <div class="gallery__swiper gallery__swiper--main swiper">
                      <button class="gallery__swiper-button gallery__swiper-button--prev" id="gallery-swiper-{{gallerySwiperCnt}}-prev" type="button">
                        <svg class="gallery__swiper-button-icon" width="11" height="17" viewBox="0 0 11 17">
                          <use xlink:href="img/sprite.svg#swiper-arrow-icon"></use>
                        </svg>
                        <span class="visually-hidden">Перелестнуть назад</span>
                      </button>
                      <button class="gallery__swiper-button gallery__swiper-button--next" id="gallery-swiper-{{gallerySwiperCnt}}-next" type="button">
                        <svg class="gallery__swiper-button-icon" width="11" height="17" viewBox="0 0 11 17">
                          <use xlink:href="img/sprite.svg#swiper-arrow-icon"></use>
                        </svg>
                        <span class="visually-hidden">Перелестнуть вперед</span>
                      </button>
                      <div class="swiper-wrapper">
                        {% for imageCount in range(SLIDES_NUM) %}
                          <div class="gallery__swiper-slide swiper-slide">
                            <picture class="gallery__swiper-slide-wrapper">
                              <source type="image/webp" srcset="https://loremflickr.com/1920/1080/gym,girl/all?{{imageCount + 1}} 1x, https://loremflickr.com/1920/1080/gym,girl/all?{{imageCount + 1}} 2x">
                              <img class="gallery__image" src="https://loremflickr.com/1920/1080/gym,girl/all?{{imageCount + 1}}" srcset="https://loremflickr.com/1920/1080/gym,girl/all?{{imageCount + 1}} 2x" alt="Описание фото {{imageCount + 1}}" loading="lazy" width="1170" height="658">
                            </picture>
                          </div>
                        {% endfor %}
                      </div>
                    </div>
                    <div class="gallery__swiper gallery__swiper--thumbs swiper">
                      <div class="swiper-wrapper">
                        {% for imageCount in range(SLIDES_NUM) %}
                          <div class="gallery__swiper-thumb swiper-slide">
                            <picture class="gallery__swiper-thumb-wrapper">
                              <source type="image/webp" srcset="https://loremflickr.com/1920/1080/gym,girl/all?{{imageCount + 1}} 1x, https://loremflickr.com/1920/1080/gym,girl/all?{{imageCount + 1}} 2x">
                              <img class="gallery__image" src="https://loremflickr.com/1920/1080/gym,girl/all?{{imageCount + 1}}" srcset="https://loremflickr.com/1920/1080/gym,girl/all?{{imageCount + 1}} 2x" alt="Описание фото {{imageCount + 1}}" loading="lazy" width="146" height="100">
                            </picture>
                          </div>
                        {% endfor %}
                      </div>
                    </div>
                  </div>
                </div>
                {% set gallerySwiperCnt = gallerySwiperCnt + 1 %}
              </div>
            </div>
            <div class="gym-info__panel tabs__panel" role="tabpanel" id="crossfit-studio">
              <h3 class="visually-hidden">Студия кроссфита</h3>
              <div class="gym-info__panel-wrapper">
                <div class="gym-info__panel-main">
                  <span class="gym-info__bg-text">Крос</span>
                  <p class="gym-info__panel-title">
                    <span>В зоне кроссфита Powerhouse Gym</span> есть все для работы со своим весом
                  </p>
                  <p class="gym-info__panel-text">
                    Десятки турников, брусьев и перекладин. Кольца, тяжелые колеса, канаты и молот
                  </p>
                  <div class="gym-info__panel-buttons">
                    <a class="button button--reverse" href="#!">Записаться на пробную тренировку</a>
                    <a class="button" href="./crossfit.html">
                      <span>Подробнее о зоне кроссфита</span>
                      <svg class="button__icon" width="8" height="14" viewBox="0 0 8 14">
                        <use xlink:href="img/sprite.svg#button-arrow-icon"></use>
                      </svg>
                    </a>
                  </div>
                </div>
                <div class="gym-info__panel-gallery gallery">
                  {% set SLIDES_NUM = 10 %}
                  <div class="gallery__swiper-wrapper">
                    <div class="gallery__swiper gallery__swiper--main swiper">
                      <button class="gallery__swiper-button gallery__swiper-button--prev" id="gallery-swiper-{{gallerySwiperCnt}}-prev" type="button">
                        <svg class="gallery__swiper-button-icon" width="11" height="17" viewBox="0 0 11 17">
                          <use xlink:href="img/sprite.svg#swiper-arrow-icon"></use>
                        </svg>
                        <span class="visually-hidden">Перелестнуть назад</span>
                      </button>
                      <button class="gallery__swiper-button gallery__swiper-button--next" id="gallery-swiper-{{gallerySwiperCnt}}-next" type="button">
                        <svg class="gallery__swiper-button-icon" width="11" height="17" viewBox="0 0 11 17">
                          <use xlink:href="img/sprite.svg#swiper-arrow-icon"></use>
                        </svg>
                        <span class="visually-hidden">Перелестнуть вперед</span>
                      </button>
                      <div class="swiper-wrapper">
                        {% for imageCount in range(SLIDES_NUM) %}
                          <div class="gallery__swiper-slide swiper-slide">
                            <picture class="gallery__swiper-slide-wrapper">
                              <source type="image/webp" srcset="https://loremflickr.com/1920/1080/gym,girl/all?{{imageCount + 1}} 1x, https://loremflickr.com/1920/1080/gym,girl/all?{{imageCount + 1}} 2x">
                              <img class="gallery__image" src="https://loremflickr.com/1920/1080/gym,girl/all?{{imageCount + 1}}" srcset="https://loremflickr.com/1920/1080/gym,girl/all?{{imageCount + 1}} 2x" alt="Описание фото {{imageCount + 1}}" loading="lazy" width="1170" height="658">
                            </picture>
                          </div>
                        {% endfor %}
                      </div>
                    </div>
                    <div class="gallery__swiper gallery__swiper--thumbs swiper">
                      <div class="swiper-wrapper">
                        {% for imageCount in range(SLIDES_NUM) %}
                          <div class="gallery__swiper-thumb swiper-slide">
                            <picture class="gallery__swiper-thumb-wrapper">
                              <source type="image/webp" srcset="https://loremflickr.com/1920/1080/gym,girl/all?{{imageCount + 1}} 1x, https://loremflickr.com/1920/1080/gym,girl/all?{{imageCount + 1}} 2x">
                              <img class="gallery__image" src="https://loremflickr.com/1920/1080/gym,girl/all?{{imageCount + 1}}" srcset="https://loremflickr.com/1920/1080/gym,girl/all?{{imageCount + 1}} 2x" alt="Описание фото {{imageCount + 1}}" loading="lazy" width="146" height="100">
                            </picture>
                          </div>
                        {% endfor %}
                      </div>
                    </div>
                  </div>
                </div>
                {% set gallerySwiperCnt = gallerySwiperCnt + 1 %}
              </div>
            </div>
            <div class="gym-info__panel tabs__panel" role="tabpanel" id="cycle-studio">
              <h3 class="visually-hidden">Студия сайкл</h3>
              <div class="gym-info__panel-wrapper">
                <div class="gym-info__panel-main">
                  <span class="gym-info__bg-text">Трен</span>
                  <p class="gym-info__panel-title">
                    <span>Групповые тренировки Сайкл</span> по программам Les Mills
                  </p>
                <div class="gym-info__panel-logos">
                    <div class="gym-info__panel-logos-item">
                      <picture>
                        <source type="image/webp" srcset="@img/cycle-lesmills-rpm-logo.webp 1x">
                        <img class="gym-info__panel-logo-image" src="@img/cycle-lesmills-rpm-logo.png" alt="Логотип lesmills rpm" width="117" height="46">
                      </picture>
                    </div>
                    <div class="gym-info__panel-logos-item">
                      <picture>
                        <source type="image/webp" srcset="@img/cycle-lesmills-sprint-logo.webp 1x">
                        <img class="gym-info__panel-logo-image" src="@img/cycle-lesmills-sprint-logo.png" alt="Логотип lesmills sprint" width="144" height="55">
                      </picture>
                    </div>
                    <div class="gym-info__panel-logos-item">
                      <picture>
                        <source type="image/webp" srcset="@img/cycle-lesmills-trip-logo.webp 1x">
                        <img class="gym-info__panel-logo-image" src="@img/cycle-lesmills-trip-logo.png" alt="Логотип lesmills the trip" width="188" height="49">
                      </picture>
                    </div>
                  </div>
                  <p class="gym-info__panel-text">
                    Каждая тренировка - шоу и соревнование, а тренер - ведущий. Пока вы путешествуете на велосипеде по вымышленным мирам, ваше тело тело становится сильнее
                  </p>
                  <ul class="gym-info__panel-list list">
                    <li class="list__item"><b>Доступна членам клуба с 7 утра до 12 ночи</b></li>
                  </ul>
                  <div class="gym-info__panel-buttons">
                    <a class="button button--reverse" href="#!">Записаться на пробную тренировку</a>
                    <a class="button" href="./cycle.html">
                      <span>Подробнее о сайкле</span>
                      <svg class="button__icon" width="8" height="14" viewBox="0 0 8 14">
                        <use xlink:href="img/sprite.svg#button-arrow-icon"></use>
                      </svg>
                    </a>
                  </div>
                </div>
                <div class="gym-info__panel-gallery gallery">
                  {% set SLIDES_NUM = 10 %}
                  <div class="gallery__swiper-wrapper">
                    <div class="gallery__swiper gallery__swiper--main swiper">
                      <button class="gallery__swiper-button gallery__swiper-button--prev" id="gallery-swiper-{{gallerySwiperCnt}}-prev" type="button">
                        <svg class="gallery__swiper-button-icon" width="11" height="17" viewBox="0 0 11 17">
                          <use xlink:href="img/sprite.svg#swiper-arrow-icon"></use>
                        </svg>
                        <span class="visually-hidden">Перелестнуть назад</span>
                      </button>
                      <button class="gallery__swiper-button gallery__swiper-button--next" id="gallery-swiper-{{gallerySwiperCnt}}-next" type="button">
                        <svg class="gallery__swiper-button-icon" width="11" height="17" viewBox="0 0 11 17">
                          <use xlink:href="img/sprite.svg#swiper-arrow-icon"></use>
                        </svg>
                        <span class="visually-hidden">Перелестнуть вперед</span>
                      </button>
                      <div class="swiper-wrapper">
                        {% for imageCount in range(SLIDES_NUM) %}
                          <div class="gallery__swiper-slide swiper-slide">
                            <picture class="gallery__swiper-slide-wrapper">
                              <source type="image/webp" srcset="https://loremflickr.com/1920/1080/gym,girl/all?{{imageCount + 1}} 1x, https://loremflickr.com/1920/1080/gym,girl/all?{{imageCount + 1}} 2x">
                              <img class="gallery__image" src="https://loremflickr.com/1920/1080/gym,girl/all?{{imageCount + 1}}" srcset="https://loremflickr.com/1920/1080/gym,girl/all?{{imageCount + 1}} 2x" alt="Описание фото {{imageCount + 1}}" loading="lazy" width="1170" height="658">
                            </picture>
                          </div>
                        {% endfor %}
                      </div>
                    </div>
                    <div class="gallery__swiper gallery__swiper--thumbs swiper">
                      <div class="swiper-wrapper">
                        {% for imageCount in range(SLIDES_NUM) %}
                          <div class="gallery__swiper-thumb swiper-slide">
                            <picture class="gallery__swiper-thumb-wrapper">
                              <source type="image/webp" srcset="https://loremflickr.com/1920/1080/gym,girl/all?{{imageCount + 1}} 1x, https://loremflickr.com/1920/1080/gym,girl/all?{{imageCount + 1}} 2x">
                              <img class="gallery__image" src="https://loremflickr.com/1920/1080/gym,girl/all?{{imageCount + 1}}" srcset="https://loremflickr.com/1920/1080/gym,girl/all?{{imageCount + 1}} 2x" alt="Описание фото {{imageCount + 1}}" loading="lazy" width="146" height="100">
                            </picture>
                          </div>
                        {% endfor %}
                      </div>
                    </div>
