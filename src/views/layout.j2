{% set tabletWidth = "748px"%}
{% set laptopWidth = "1020px"%}
{% set desktopWidth = "1707px"%}

<!DOCTYPE html>
<html class="page" lang="ru">
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>{{ site_name }}</title>
  {# <link rel="icon" href="favicon.ico"> #}
  <link rel="stylesheet" href="css/style.min.css">
</head>
<body class="page__body">
  <div class="page__body-wrapper">
    {% include "header.j2" %}

    {% block content %}{% endblock %}

    {% include "footer.j2" %}

    {% include "./blocks/modal-fit-test.j2" %}
    {% include "./blocks/modal-backcall.j2" %}

    {% block modal %}{% endblock %}
  </div>



  <script src="./js/imask.js"></script>
  <script src="./js/pristine.min.js"></script>
  <script src="./js/global.min.js"></script>
  <script src="./js/burger.min.js"></script>
  <script src="./js/searchbar.min.js"></script>
  <script src="./js/phone-mask.min.js"></script>
  <script src="./js/modal.min.js"></script>
  {% block js %}{% endblock %}
</body>
</html>
