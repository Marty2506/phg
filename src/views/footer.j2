<footer class="page-footer">
  <div class="page-footer__container container">
    <div class="page-footer__main">
      <div class="page-footer__main-wrapper">
        <div class="page-footer__logos">
          <a class="page-footer__logo logo" href="./index.html" >
            <img class="page-footer__logo-image logo__image" src="@img/phg-logo-header.svg" alt="Логотип POWRHOUSE GYM" width="132" height="57">
          </a>
          <div class="page-footer__logo-addr">
            <span class="page-footer__logo-addr-city">Москва, м. Сокол</span>
            <span class="page-footer__logo-addr-street">Ленинградский проспект, 80к37</span>
          </div>
        </div>
        <nav class="page-footer__nav">
          {% for group in footerNav %}
            <div class="page-footer__nav-group">
              <ul class="page-footer__nav-list">
                {% for link in group %}
                  <li class="page-footer__nav-item">
                    <a class="page-footer__link" href="{{ link.href }}">{{ link.title | safe}}</a>
                  </li>
                {% endfor %}
              </ul>
            </div>
          {% endfor %}
        </nav>
        <div class="page-footer__download">
          <span class="page-footer__download-title">Скачайте приложение</span>
          <ul class="page-footer__download-list">
            <li class="page-footer__download-item">
              <a class="page-footer__download-link" href="#!">
                <svg class="page-footer__download-link-icon" width="18" height="23" viewBox="0 0 18 23">
                  <use xlink:href="img/sprite.svg#appstore-icon"></use>
                </svg>
                <span class="page-footer__download-link-text">в App Store</span>
              </a>
            </li>
            <li class="page-footer__download-item">
              <a class="page-footer__download-link" href="#!">
                <svg class="page-footer__download-link-icon" width="19" height="19" viewBox="0 0 19 19">
                  <use xlink:href="img/sprite.svg#googleplay-icon"></use>
                </svg>
                <span class="page-footer__download-link-text">в Google Play</span>
              </a>
            </li>
          </ul>
        </div>
      </div>
      <div class="page-footer__contacts">
        <div class="page-footer__phone-wrapper">
          <div class="page-footer__messengers">
            <a class="page-footer__messenger messenger messenger--telegram" href="#!">
              <svg class="messenger__icon" width="16" height="14" viewBox="0 0 16 14">
                <use xlink:href="img/sprite.svg#telegram-icon"></use>
              </svg>
              <span class="visually-hidden">Контакт телеграма</span>
            </a>
            <a class="page-footer__messenger messenger messenger--whatsapp" href="#!">
              <svg class="messenger__icon" width="16" height="17" viewBox="0 0 16 17">
                <use xlink:href="img/sprite.svg#whatsapp-icon"></use>
              </svg>
              <span class="visually-hidden">Контакт вотсапа</span>
            </a>
          </div>
          <a class="page-footer__phone" href="tel:+79032038690">
            <svg class="page-header__phone-icon" width="25" height="26" viewBox="0 0 25 26">
              <use xlink:href="img/sprite.svg#tel-icon"></use>
            </svg>
            <span>+7 (903) 203-86-90</span>
          </a>
        </div>
        <button class="button button--reverse page-footer__backcall-button">Обратный звонок</button>
        <div class="page-footer__social">
          <span class="page-footer__social-title">Социальные сети</span>
          <a class="menu__messenger messenger messenger--vk messenger--red" href="#!">
            <svg class="messenger__icon" width="23" height="14" viewBox="0 0 23 14">
              <use xlink:href="img/sprite.svg#vk-icon"></use>
            </svg>
            <span class="visually-hidden">Мы в вк.</span>
          </a>
          <a class="menu__messenger messenger messenger--youtube messenger--red" href="#!">
            <svg class="messenger__icon" width="23" height="27" viewBox="0 0 23 27" >
              <use xlink:href="img/sprite.svg#youtube-icon"></use>
            </svg>
            <span class="visually-hidden">Мы в youtube.</span>
          </a>
          <a class="menu__messenger messenger messenger--insta messenger--red" href="#!">
            <svg class="messenger__icon" width="26" height="27" viewBox="0 0 26 27">
              <use xlink:href="img/sprite.svg#inst-icon"></use>
            </svg>
            <span class="visually-hidden">Мы в instagram.</span>
          </a>
        </div>
      </div>
    </div>
    <div class="page-footer__other">
      <ul class="page-footer__other-list">
        <li class="page-footer__other-list-item">
          <a class="page-footer__other-link" href="#!">Политика конфиденциальности</a>
        </li>
        <li class="page-footer__other-list-item">
          <a class="page-footer__other-link" href="#!">Правила клуба</a>
        </li>
        <li class="page-footer__other-list-item">
          <a class="page-footer__other-link" href="#!">Договор</a>
        </li>
      </ul>
      <div class="page-footer__dev">
        <span class="page-footer__dev-title">Разработка сайта — </span>
        <a class="page-footer__dev-link" href="#!">Бондарь Роман</a>
      </div>
    </div>
  </div>
</footer>
